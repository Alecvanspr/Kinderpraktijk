@{
    ViewData["Title"] = "Clients";
}
@model List<Chat>
<h1 class="text-center">Uw cliÃ«nten</h1>
<p style="font-size:large" class="text-center">Dit zijn de clienten die bij u staan ingeschreven</p>
<form class="filter-box">
    <table>
        <tbody>
        <tr>
            <td><label for="zoekBalkNaam">Zoek op naam</label></td>
            <td><input id="zoekBalkNaam" type="text" placeholder="Naam groep"/></td>
        </tr>
        </tbody>
    </table>
    <input type="submit" value="Zoek" alt="Zoek in lijst van groepen" class="btn btn-primary">
</form>

<div class="groups b-large">
    @if(Model!=null){
    @foreach (var client in Model)
        {
            <div class="group">
                <h2>@client.Naam</h2>
                <a class="btn btn-primary">Chat</a>
            </div>
        }
    }
</div>